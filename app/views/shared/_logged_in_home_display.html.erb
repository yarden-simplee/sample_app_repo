<div class="row">
    <aside class="col-md-4">
      <section class="user_info">
      	<%# We need to specify to look for partials in the 'shared' directory, since rails' default directory in this case will be 'views' %>
        <%= render 'shared/user_info' %> <%# A partial %>
      </section>
      <section class="stats">
      	<%= render 'shared/stats' %>
      </section>
      <section class="micropost_form">
        <%= render 'shared/micropost_form' %> <%# A partial %>
      </section>
    </aside>
    <div class="col-md-8">
      <h3>Micropost Feed</h3>
      <%= render 'shared/feed' %> <%# A partial %>
    </div>
</div>